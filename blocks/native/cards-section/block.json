{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "native/cards-section",
  "version": "0.1.0",
  "title": "KalaPress - Cards",
  "category": "kalapress",
  "icon": "screenoptions",
  "description": "Display content within a cards section. Contains a section header, grid of cards, and section footer.",
  "keywords": [
    "cards",
    "cards section",
    "featured cards"
  ],
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "margin": [ "top", "bottom"],
      "padding": true
    }
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    }
  },
  "example": {
    "viewportWidth": 960
  },
	"textdomain": "native",
	"editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
  "render": "file:./render.php"
}