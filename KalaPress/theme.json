        "border": {
          "color": "transparent",
          "width": "2px",
          "radius": "3px"
        }
      },
      "caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
    },
    "blocks": {
      "core/columns": {
        "spacing": {
          "blockGap": "3.5%"
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "1.5em"
          }
        }
      },
      "core/button": {
        "color": {
          "background": "var(--wp--preset--color--secondary)",
          "text": "var(--wp--preset--color--base)"
        },
        "typography": {
