{
  "name": "kalapress",
  "author": "Kalamuna",
  "description": "A WordPress starter theme",
  "keywords": [
    "Kalapress",
    "WordPress",
    "WordPress Starter",
    "WordPress Starter Theme",
    "WordPress Hybrid Theme",
    "Theme",
    "Gutenberg",
    "Blocks",
    "Kalamuna",
    "Kalapress",
    "Starter",
    "Starter Theme",
    "Starter Kit"
  ],
  "devDependencies": {
    "@wordpress/block-editor": "^12.12.0",
    "@wordpress/components": "^25.10.0",
    "@wordpress/scripts": "^26.15.0",
    "@wordpress/stylelint-config": "^21.27.0",
    "breakpoint-sass": "^2.7.1",
    "browser-sync": "^2.27.11",
    "dir-archiver": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start:blocks:acf": "wp-scripts start --webpack-src-dir=blocks/acf --webpack-copy-php --output-path=build/blocks/acf",
    "start:blocks:native": "wp-scripts start --webpack-src-dir=blocks/native --webpack-copy-php --output-path=build/blocks/native",
    "start:blocks": "wp-scripts start --webpack-src-dir=blocks --webpack-copy-php --output-path=build/blocks",
    "build:blocks": "wp-scripts build --webpack-src-dir=blocks --webpack-copy-php --output-path=build/blocks",
    "build:blocks:acf": "wp-scripts build --webpack-src-dir=blocks/acf --webpack-copy-php --output-path=build/blocks/acf",
    "build:blocks:native": "wp-scripts build --webpack-src-dir=blocks/native --webpack-copy-php --output-path=build/blocks/native",
    "start:admin:scripts": "node bin/build-script.js start admin:scripts",
    "start:editor:scripts": "node bin/build-script.js start editor:scripts",
    "start:theme:scripts": "node bin/build-script.js start theme:scripts",
    "start:admin:styles": "node bin/build-script.js start admin:styles",
    "start:editor:styles": "node bin/build-script.js start editor:styles",
    "start:theme:styles": "node bin/build-script.js start theme:styles",
    "build:admin:scripts": "node bin/build-script.js build admin:scripts",
    "build:editor:scripts": "node bin/build-script.js build editor:scripts",
    "build:theme:scripts": "node bin/build-script.js build theme:scripts",
    "build:admin:styles": "node bin/build-script.js build admin:styles",
    "build:editor:styles": "node bin/build-script.js build editor:styles",
    "build:theme:styles": "node bin/build-script.js build theme:styles",
    "postbuild:theme:styles": "rimraf build/theme/styles/**/*.js",
    "postbuild:admin:styles": "rimraf build/admin/styles/**/*.js",
    "postbuild:editor:styles": "rimraf build/editor/styles/**/*.js",
    "start": "npm-run-all --parallel start:admin start:editor start:theme start:blocks",
    "build": "npm-run-all --parallel build:admin build:editor build:theme build:blocks",
    "build:admin": "npm-run-all --parallel \"build:admin:scripts -- all\" \"build:admin:styles -- all\"",
    "start:admin": "npm-run-all --parallel \"start:admin:scripts -- all\" \"start:admin:styles -- all\"",
    "build:editor": "npm-run-all --parallel \"build:editor:scripts -- all\" \"build:editor:styles -- all\"",
    "start:editor": "npm-run-all --parallel \"start:editor:scripts -- all\" \"start:editor:styles -- all\"",
    "build:theme": "npm-run-all --parallel \"build:theme:scripts -- current\" \"build:theme:styles -- current\"",
    "start:theme": "npm-run-all --parallel \"start:theme:scripts -- current\" \"start:theme:styles -- current\"",
    "lint:scss:blocks": "wp-scripts lint-style blocks/**/*.scss",
    "lint:scss:theme": "wp-scripts lint-style src/**/*.scss",
    "lint:js:blocks": "wp-scripts lint-js blocks/**/*.js",
    "lint:js:theme": "wp-scripts lint-js src/**/*.js",
    "lint:scss": "wp-scripts lint-style src/**/*.scss blocks/**/*.scss",
    "lint:js": "wp-scripts lint-js src/**/*.js blocks/**/*.js",
    "format:blocks": "wp-scripts format ./blocks",
    "format:theme": "wp-scripts format ./src",
    "format": "wp-scripts format ./src ./blocks",
    "scaffold:block:dynamic": "if [ \"$(basename $(pwd))\" != \"native\" ]; then cd blocks/native; fi && npx @wordpress/create-block --namespace native --variant dynamic --no-plugin",
    "scaffold:block:static": "if [ \"$(basename $(pwd))\" != \"native\" ]; then cd blocks/native; fi && npx @wordpress/create-block --namespace native --variant static --no-plugin",
    "scaffold:block:acf": "node bin/create-block-wrapper.js"
  }
}
